		"tailwind sticky",
		"tailwind apexcharts",
		"tailwind clipboard"
	],
	"author": "@keenthemes",
	"types": "lib/esm/index.d.ts",
	"main": "lib/cjs/index.js",
	"module": "lib/esm/index.js",
	"license": "MIT",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "concurrently \"npm run build:webpack:watch\" \"npm run build:css\"",
		"build:webpack": "webpack --mode=development",
		"build:webpack:watch": "webpack --mode=development --watch",
		"build:prod": "webpack --env production --mode=production",
		"build:lib": "tsc --outDir lib/cjs && tsc -m es6 --outDir lib/esm",
		"build:css": "npx @tailwindcss/cli -i ./styles/main.css -o ./dist/styles.css --watch",
		"lint": "eslint src --ext .ts",
		"format": "prettier --write src"
	},
	"devDependencies": {
		"@babel/core": "^7.27.3",
		"@babel/preset-env": "^7.27.2",
		"@eslint/js": "^9.27.0",
		"@types/node": "^22.15.23",
		"autoprefixer": "^10.4.21",
		"babel-loader": "^10.0.0",
		"clean-webpack-plugin": "^4.0.0",
		"concurrently": "^9.1.2",
		"eslint": "^9.27.0",
		"mini-svg-data-uri": "^1.4.4",
		"prettier": "^3.5.3",
		"source-map-loader": "^5.0.0",
		"tailwindcss": "^4.1.7",
		"terser-webpack-plugin": "^5.3.14",
		"ts-loader": "^9.5.2",
		"typescript": "^5.8.3",
		"webpack": "^5.99.9",
		"webpack-cli": "^6.0.1",
		"webpack-merge-and-include-globally": "^2.3.4"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.8"
	}
}
