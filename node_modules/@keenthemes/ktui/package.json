		"tailwind react",
		"tailwind nextjs",
		"tailwind vue",
		"tailwind angular",
		"tailwind laravel",
		"tailwind menu",
		"tailwind megamenu",
		"tailwind buttons",
		"tailwind forms",
		"tailwind tabs",
		"tailwind datatable",
		"tailwind scrollspy",
		"tailwind scrollto",
		"tailwind stepper",
		"tailwind accordion",
		"tailwind collapse",
		"tailwind dismiss",
		"tailwind toggle",
		"tailwind reparent",
		"tailwind theme",
		"tailwind drawer",
		"tailwind modal",
		"tailwind tooltip",
		"tailwind popover",
